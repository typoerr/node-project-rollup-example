{
  "extends": [
    "@typoerr/eslint-config",
    "@typoerr/eslint-config/+typescript"
  ],
  "ignorePatterns": [
    "**/public/**",
    "**/dist/**",
    "**/node_modules/**"
  ],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/camelcase": 0
  }
}